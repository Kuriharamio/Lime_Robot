<?xml version="1.0" ?>
<robot name="omni_robot">
   <!-- Robot Footprint -->
   <!-- <link name="base_footprint"/>

   <joint name="base_joint" type="fixed">
     <parent link="base_footprint"/>
     <child link="base_link"/>
     <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
   </joint> -->

    <!-- base link -->
    <link name="base_link">
        <visual>
            <origin xyz=" 0 0 0.07" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.05" radius="0.11"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 0.5"/>
            </material>
        </visual>
        
        <collision>
            <origin xyz=" 0 0 0.07" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.05" radius="0.11"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 0.5"/>
            </material>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <inertia ixx="0.0122666" ixy="0.0" ixz="0.0" iyy="0.01222666" iyz="0.0" izz="0.02"/>
        </inertial>
    </link>   

    <!-- front_wheel link -->
    <link name="front_wheel">
        <visual>
            <origin rpy="0 1.57 0" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 1.57 0" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0.8 1"/>
            </material>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <inertia ixx="0.000190416666667" ixy="0" ixz="0" iyy="0.0001904" iyz="0" izz="0.00036"/>
        </inertial>
    </link>

    <!-- left_wheel link -->
    <link name="left_wheel">
        <visual>
            <origin rpy="0 1.57 -1.05" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 1.57 -1.05" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 0.8 1"/>
            </material>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <inertia ixx="0.000190416666667" ixy="0" ixz="0" iyy="0.0001904" iyz="0" izz="0.00036"/>
        </inertial>
    </link>

    <!-- right_wheel link -->
    <link name="right_wheel">
        <visual>
            <origin rpy="0 1.57 1.05" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 0.8 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 1.57 1.05" xyz="0 0 0.0"/>
            <geometry>
                <cylinder length="0.04" radius="0.03"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 0.8 1"/>
            </material>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <inertia ixx="0.000190416666667" ixy="0" ixz="0" iyy="0.0001904" iyz="0" izz="0.00036"/>
        </inertial>
    </link>

    <!-- base_to_front_wheel joint-->
    <joint name="base_to_front_wheel" type="continuous">
        <parent link="base_link"/>
        <child link="front_wheel"/>
        <origin rpy="0 0 0" xyz="0.1 0 0.03"/>
        <axis xyz="1 0.0 0.0"/>
    </joint>
    <!-- base_to_left_wheel joint-->
    <joint name="base_to_left_wheel" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin rpy="0 0 0" xyz="-0.055 0.09 0.03"/>
        <axis xyz="-1.0 1.732 0.0"/>
    </joint>
    <!-- base_to_right_wheel joint-->
    <joint name="base_to_right_wheel" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin rpy="0 0 0" xyz="-0.055 -0.09 0.03"/>
        <axis xyz="1.0 1.732 0.0"/>
    </joint>

    <!-- laser link -->
    <link name="laser_link">
        <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 0.5" /> 
        </material>
        </visual>
        <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder length="0.02" radius="0.02"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 0.5" /> 
        </material>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.000190416666667" ixy="0" ixz="0" iyy="0.0001904" iyz="0" izz="0.00036"/>
        </inertial>
    </link>

    <!-- laser joint -->
    <joint name="laser_joint" type="fixed">
        <parent link="base_link" />
        <child link="laser_link" />
        <origin xyz="0 0 0.105" rpy="0 0 0.80"/>
    </joint>

    <!--imu_link-->
    <link name="imu_link">
        <visual>
        <origin xyz="0 0 0.0" rpy="0 0 0"/>
        <geometry>
              <box size="0.02 0.02 0.02"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0.0" rpy="0 0 0"/>
        <geometry>
              <box size="0.02 0.02 0.02"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.1"/>
          <inertia ixx="0.000190416666667" ixy="0" ixz="0" iyy="0.0001904" iyz="0" izz="0.00036"/>
        </inertial>
    </link>

    <!-- imu joint -->
    <joint name="imu_joint" type="fixed">
        <parent link="base_link" />
        <child link="imu_link" />
        <origin xyz="0 0 0.07" />
    </joint>

    </robot>